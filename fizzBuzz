const fizzBuzz = (num1, num2) => {
  
  for(let i = num1; i <= num2; i += 1) {
    (i%3 === 0 && i %5 === 0) ? console.log('FizzBuzz') : 
    (i%3 === 0) ? console.log('Fizz') : 
    (i%5 === 0) ? console.log('Buzz') : 
    console.log(i);
  };
};

fizzBuzz(10, 20)

=>
Buzz
11
Fizz
13
14
FizzBuzz
16
17
Fizz
19
Buzz
